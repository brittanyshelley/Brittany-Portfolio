
{
  "name": "brittany-portfolio",
  "version": "1.0.0",
  "description": "Brittany-Portfolio",
  "source": "src/index.html",
  "homepage": "https://brittanyshelley.github.io/Brittany-Portfolio",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build-gh": "parcel build --public-url 'https://brittanyshelley.github.io/Brittany-Portfolio'",
    "predeploy": "npm run build-gh",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brittanyshelley/Brittany-Portfolio.git"
  },
  "author": "Brittany Shelley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brittanyshelley/Brittany-Portfolio/issues"
  },
  "dependencies": {
    "@tsparticles/react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tsparticles": "^2.12.2",
    "tsparticles": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

